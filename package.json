{
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@hotwired/turbo-rails": "^7.1.1",
    "@rails/request.js": "^0.0.6"
  },
  "scripts": {
    "jswatch": "npm run jsbuild -- --watch",
    "jsbuild": "esbuild app/assets/javascripts/playground/application.js --bundle --minify  --outfile=vendor/javascripts/playground/application.js",
